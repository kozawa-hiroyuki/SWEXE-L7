<h1>Products#index</h1>
<p>Find me in app/views/products/index.html.erb</p>


<table>
	<% @products.each do |e| %>
	<tr>
		<td><%= e.name %></td>
		<td>
			<%= link_to '表示', "/products/#{e.id}" %>
			<%= link_to '削除', "/products/#{e.id}", method: :delete %>
			<%= link_to '編集', "/products/#{e.id}/edit" %>
		</td>
	</tr>
	<% end %>
</table>

<%= link_to '新規追加', '/products/new' %>